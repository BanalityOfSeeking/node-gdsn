<tradeItem>

    <gtin/>

    <additionalTradeItemIdentification/>

    <isTradeItemABaseUnit/>
    <isTradeItemAConsumerUnit/>
    <isTradeItemADespatchUnit/>
    <isTradeItemAnInvoiceUnit/>
    <isTradeItemAnOrderableUnit/>
    <tradeItemUnitDescriptorCode/>

    <informationProviderOfTradeItem>
        <gln/>
        <partyName/>
        <partyRoleCode>INFORMATION_PROVIDER</partyRoleCode>
    </informationProviderOfTradeItem>

    <gdsnTradeItemClassification>
        <gpcCategoryCode>TOKEN</gpcCategoryCode>
        <gpcCategoryName>TOKEN</gpcCategoryName>
        <gDSNTradeItemClassificationAttribute>
            <gpcAttributeTypeCode>TOKEN</gpcAttributeTypeCode>
            <gpcAttributeValueCode>TOKEN</gpcAttributeValueCode>
            <!--
            <gpcAttributeTypeDefinition>TOKEN</gpcAttributeTypeDefinition>
            <gpcAttributeTypeName>TOKEN</gpcAttributeTypeName>
            <gpcAttributeValueName>TOKEN</gpcAttributeValueName>
            -->
        </gDSNTradeItemClassificationAttribute>
        <additionalTradeItemClassification>
            <additionalTradeItemClassificationSystemCode>TOKEN</additionalTradeItemClassificationSystemCode>
            <additionalTradeItemClassificationValue><additionalTradeItemClassificationCodeValue>TOKEN</additionalTradeItemClassificationCodeValue></additionalTradeItemClassificationValue>
        </additionalTradeItemClassification>
    </gdsnTradeItemClassification>

    <nextLowerLevelTradeItemInformation>
        <quantityOfChildren/>
        <totalQuantityOfNextLowerLevelTradeItem/>

        <childTradeItem>
            <gtin/>
            <quantityOfNextLowerLevelTradeItem/>
        </childTradeItem>

    </nextLowerLevelTradeItemInformation>

    <targetMarket>
        <targetMarketCountryCode/>
        <targetMarketSubdivisionCode/>
    </targetMarket>

    <!-- zero to many, repeated for each variant -->
    <tradeItemInformation>

        <productionVariantDescription/>
        <productionVariantEffectiveDateTime/>
        <!--tradeItemComponents/-->
        
        <extension>

            <delivery_purchasing_information:deliveryPurchasingInformationModule>
                <deliveryPurchasingInformation>
                    <firstShipDateTime/>
                </deliveryPurchasingInformation>
            </delivery_purchasing_information:deliveryPurchasingInformationModule>

            <trade_item_description:tradeItemDescriptionModule>
                <tradeItemDescriptionInformation>
                    <additionalTradeItemDescription/>
                    <descriptionShort/>
                    <functionalName/>
                    <invoiceName/>
                    <labelDescription/>
                    <productRange/>
                    <tradeItemDescription/>
                    <tradeItemFormDescription/>
                    <tradeItemGroupIdentificationCodeReference/>
                    <variantDescription/>
                    <brandNameInformation>
                        <brandName/>
                        <languageSpecificBrandName/>
                        <languageSpecificSubbrandName/>
                        <subBrand/>
                    </brandNameInformation>
                </tradeItemDescriptionInformation>
            </trade_item_description:tradeItemDescriptionModule>

            <trade_item_measurements:tradeItemMeasurementsModule>
                <tradeItemMeasurements>
                    <depth measurementUnitCode="CM" codeListVersion="1">25</depth>
                    <height measurementUnitCode="CM" codeListVersion="1">10</height>
                    <netContent measurementUnitCode="LB" codeListVersion="1">100</netContent>
                    <width measurementUnitCode="CM" codeListVersion="1">25</width>
                    <tradeItemWeight>
                        <grossWeight measurementUnitCode="LB" codeListVersion="1">100</grossWeight>
                        <netWeight measurementUnitCode="LB" codeListVersion="1">100</netWeight>
                    </tradeItemWeight>
                </tradeItemMeasurements>
            </trade_item_measurements:tradeItemMeasurementsModule>

            <variable_trade_item_information:variableTradeItemInformationModule>
                <variableTradeItemInformation>
                    <isTradeItemAVariableUnit>false</isTradeItemAVariableUnit>
                </variableTradeItemInformation>
            </variable_trade_item_information:variableTradeItemInformationModule>

            <food_and_beverage_ingredient:foodAndBeverageIngredientModule>
                <additiveInformation>
                    <additiveName>Sodium Nitrate</additiveName>
                    <levelOfContainmentCode>CONTAINS</levelOfContainmentCode>
                </additiveInformation>
                <foodAndBeverageIngredient>
                    <ingredientSequence>1</ingredientSequence>
                    <ingredientContentPercentage>80.00</ingredientContentPercentage>
                    <ingredientName languageCode="en">Milk</ingredientName>
                </foodAndBeverageIngredient>
                <foodAndBeverageIngredient>
                    <ingredientSequence>2</ingredientSequence>
                    <ingredientContentPercentage>20.00</ingredientContentPercentage>
                    <ingredientName languageCode="en">Sugar</ingredientName>
                </foodAndBeverageIngredient>
            </food_and_beverage_ingredient:foodAndBeverageIngredientModule>

        </extension>

        <avpList/>

    </tradeItemInformation>

    <tradeItemSynchronisationDates><!-- one and only one -->
        <lastChangeDateTime>TOKEN</lastChangeDateTime>
        <cancelledDateTime>TOKEN</cancelledDateTime>
        <communityVisibilityDateTime>TOKEN</communityVisibilityDateTime>
        <discontinuedDateTime>TOKEN</discontinuedDateTime>
        <effectiveDateTime>TOKEN</effectiveDateTime>
        <publicationDateTime>TOKEN</publicationDateTime>
    </tradeItemSynchronisationDates>

</tradeItem>
